{
  "compilerOptions": {
    "checkJs": true,
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noEmit": true,
    "strict": false,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["react/canary", "bun", "@cloudflare/workers-types/2024-10-05"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
